{
  "name": "cricket-heritage-bot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "backup": "./scripts/backup-db.sh",
    "backup:s3": "./scripts/backup-to-s3.sh",
    "restore:s3": "./scripts/restore-from-s3.sh",
    "setup:s3": "./scripts/setup-s3.sh",
    "test:aws": "./scripts/test-aws-setup.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:aws": "./scripts/deploy-to-aws.sh",
    "docker:build": "docker build -t cricket-bot .",
    "docker:run": "docker run --env-file .env cricket-bot"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "axios": "^1.12.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "telegraf": "^4.16.3",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}
